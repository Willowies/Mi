<mapper namespace="com.mi.model.dao.ReceivingPointDAO">
    
    <select id="selectRP" parameterType="String" resultType="ReceivingPoint"  >
        select *  from  ReceivingPoint 
        where rpCity=#{city}
    </select>
   
